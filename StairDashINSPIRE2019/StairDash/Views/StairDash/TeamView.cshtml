
@{
    ViewBag.Title = "TeamView";
}

<h2>TeamView</h2>

<div class="w3-container">

    <div class="w3-light-grey">
        <div class="w3-light-blue" style="height:24px;width:25%"></div>
    </div><br>

    <div class="w3-light-grey">
        <div class="w3-green" style="height:24px;width:75%"></div>
    </div><br>




    <table border="1">
        <tr>
            <th>Team Name</th>
            <th>Steps</th>

        </tr>

        @foreach (var t in ViewBag.teams)
        {
            <tr>
                <td>@t.Name</td>
                <td>@t.Score</td>

            </tr>

        }
    </table>

</div>

