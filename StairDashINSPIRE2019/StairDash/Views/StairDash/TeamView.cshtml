
@{
    ViewBag.Title = "TeamView";
}

<h2>TeamView</h2>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=0.1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
</head>


<table border="1">
    <tr>
        <th>Team Name</th>
        <th>Steps</th>
        <th>________________</th>


    </tr>

    @foreach (var t in ViewBag.teams)
    {
        <tr>
            <td>@t.Name</td>
            <td>@t.Score</td>
            <td>
                <div class="w3-container">

                    <div class="w3-light-grey">
                        <div class="w3-light-blue" style="height:12px;width:25%"></div>
                    </div><br>


                </div>
            </td>

        </tr>

    }
</table>

